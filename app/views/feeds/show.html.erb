<% title "Feed" %>

<p>
  <strong>Title:</strong>
  <%= @feed.title %>
</p>
<p>
  <strong>Url:</strong>
  <%= @feed.url %>
</p>
<p>
  <strong>Updated At:</strong>
  <%= @feed.updated_at %>
</p>

<p>
  <%= link_to "Edit", edit_feed_path(@feed) %> |
  <%= link_to "Destroy", @feed, :confirm => 'Are you sure?', :method => :delete %> |
  <%= link_to "View All", feeds_path %>
</p>
